{
  "id": "http://schemas.broid.ai/object/place.json",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": ["Place"]
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "context": {
      "oneOf": [{
        "$ref": "http://schemas.broid.ai/object/context.json"
      }, {
        "type": "array",
        "items": {
          "$ref": "http://schemas.broid.ai/object/context.json"
        }
      }]
    },
    "tag": {
      "oneOf": [{
        "$ref": "http://schemas.broid.ai/embedded/tag.json"
      }, {
        "type": "array",
        "items": {
          "$ref": "http://schemas.broid.ai/embedded/tag.json"
        }
      }]
    }
  },
  "required": ["type", "latitude", "longitude"]
}